<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE smil PUBLIC "-//W3C//DTD SMIL 2.0//EN" "http://www.w3.org/TR/SMIL20/SMIL20.dtd">
<smil xmlns="http://www.w3.org/2001/SMIL20/Language">
   
  <head>

    <layout>

     <root-layout width="800px" height="600px"/> 

     <!-- texts area -->
      <region id="texte-intro-1" top="10%" left="50%" height="20px" fit="meet"/>
      <region id="texte-intro-2" top="15%" left="50%" height="20px" fit="meet"/>
      <region id="texte-intro-3" top="7%"  left="25%" height="20px" width="180px" fit="meet"/>
      <region id="texte-intro-4" top="20%" left="65%" height="50px" fit="meet"/>
      <region id="texte-intro-5" top="30%" left="65%" height="75px" fit="meet"/>
      <region id="texte-intro-6" top="45%" left="65%" height="50px" fit="meet"/>
      <region id="texte-intro-7" top="7%"  left="25%" height="20px" width="250px" fit="meet"/>
      <region id="texte-intro-8" top="17%" left="5%"  height="80px" width="250px" fit="meet"/>

      <region id="texte3" top="10%" left="50%" height="20px" fit="meet"/>
      <region id="texte4" top="10%" left="50%" height="20px" fit="meet"/>
      <region id="texte5" top="10%" left="50%" height="20px" fit="meet"/> 
     
      <region id="titre" left="70%" top="100px" width="250px" height="30" />
            
      <region id="texte1-moon" left="70%" top="200px" width="500px" height="80px" fit="meet" />
      <region id="texte2-moon" left="70%" top="280px" width="500px" height="80px" fit="meet" />
      <region id="texte3-moon" left="70%" top="380px" width="500px" height="80px" fit="meet" />
            
     <!-- audios area -->
      <region id="audio" top="35%" left="10%"/>

     <!-- images area -->
      <region id="image-intro-1" top="50%" left="5%" width="30%" fit="meet"/>
      <region id="image-intro-2" top="30%" left="50%" height="400px"/>
      <region id="image-intro-3" top="60%" left="65%" width="55%" fit="meet"/>  
      <region id="image-intro-4" top="40%" left="5%" width="58%" fit="meet"/> 
      <region id="image-intro-5" top="10%" left="75%" width=" 35%" fit="meet"/> 
      
      <region id="image3" top="50%" left="5%" width="30%" fit="meet"/>
      <region id="image4" top="50%" left="5%" width="30%" fit="meet"/>
      <region id="image5" top="50%" left="5%" width="30%" fit="meet"/>  
      
      <region id="image-moon" left="0" top="0" width="550px" height="450px"  fit="meet"/>

     <!-- videos area -->
      <region id="video-intro-1" left="5%" />
      <region id="video-intro-2" top="10%" left="5%" width="30%" fit="meet"/>
      <region id="video-intro-3" top="10%" left="5%" width="50%" fit="meet" />
      <region id="video-intro-4" top="50%" left="5%" width="50%" fit="meet" />
      <region id="video" left="0" top="200px" width="550px" height="450px"  fit="meet"/>

    </layout>

  </head>


  <body>
 
    <!-- slide1 -->
    <par>
       
      <video src="videos/video-intro-1.mp4" dur="48.2s" region="video-intro-1"/>
      <seq>

        <audio src="audios/audio-intro-1.mp3" begin="3s"/>
        <audio src="audios/audio-intro-2.mp3" begin="1s"/>
        <audio src="audios/audio-intro-3.mp3" begin="1s"/>
        <audio src="audios/audio-intro-4.mp3" />
      </seq>
    </par>
    
    <par>
      <audio src="audios/music.mp3" />
     <seq>
        <!-- slide2 -->
        <par>

          <audio src="audios/audio-intro-5.mp3" begin="3s"/>
          <text src="textes/texte-intro-1.txt" region="texte-intro-1" begin="6s"/>
          <text src="textes/texte-intro-2.txt" region="texte-intro-2" begin="10s"/>
          <video src="videos/video-intro-2.mp4" region="video-intro-2" />
          <img src="images/image-intro-1.jpg" region="image-intro-1" begin="3s"/>
          <img src="images/image-intro-2.jpg" region="image-intro-2" begin="4s"/>
        </par>

        <!-- slide3 -->
        <par>

          <seq>
            <audio src="audios/audio-intro-6.mp3" />
            <audio src="audios/audio-intro-7.mp3" />
            <audio src="audios/audio-intro-8.mp3" />
          </seq>

          <text src="textes/texte-intro-3.txt" region="texte-intro-3" />
          <text src="textes/texte-intro-4.txt" region="texte-intro-4" begin="3s"/>
          <text src="textes/texte-intro-5.txt" region="texte-intro-5" begin="21s"/>
          <text src="textes/texte-intro-6.txt" region="texte-intro-6" begin="34s"/>
          <img src="images/image-intro-3.jpg"  region="image-intro-3" begin="41s"/>
          
          <seq>
            <video src="videos/video66.mp4" region="video-intro-3" />

            <par>
            <video src="videos/video5.mp4" region="video-intro-3" begin="1s" />
            <video src="videos/video88.mp4" region="video-intro-4" begin="12s"  />
            </par>
          </seq>
        </par>

        <!-- slide4 -->
        <par>

          <seq>
            <audio src="audios/audio-intro-9.mp3" begin="1s" />
            <audio src="audios/audio-intro-10.mp3" begin="2s" />
            <audio src="audios/audio-intro-11.mp3" />
          </seq>

          <text src="textes/texte-intro-7.txt" region="texte-intro-7" begin="1s"/>
          <text src="textes/texte-intro-8.txt" region="texte-intro-8" begin="5s"/>
          <img src="images/image-intro-4.jpeg" region="image-intro-4" begin="7s"/>
          <img src="images/image-intro-5.jpg"  region="image-intro-5" begin="1s"/>
         
        </par>

        <!-- slide5- la nouvelle lune-->
        <par>
          <audio src="audios/audio6.mp3" dur="44s" begin="3s"/> 
          <text src="textes/texte3.txt" region="texte3" begin="5s"/>
          <img src="images/image3.jpg" region="image3"/> 
        </par>

        <!-- slide6- le premier croissant  -->
        <par>
          <audio src="audios/audio7.mp3" dur="40s" begin="3s"/> 
          <text src="textes/texte4.txt" region="texte4" begin="5s"/> 
          <img src="images/image4.jpg" region="image4"/> 
        </par>

        <!-- slide7-  Premier Quartier de la Lune  -->
        <par>
          <audio src="audios/audio8.mp3" dur="39s" begin="3s"/> 
          <text src="textes/texte5.txt" region="texte5" begin="5s"/> 
          <img src="images/image5.jpg" region="image5"/> 
        </par>

        <!-- lune gibbeuse croissante-->
        <par> 
                                      
          <text src="textes/GC-titre.txt" region="titre" begin="2s" dur="30s" />
          <seq>
            <text src="textes/GC-apparence.txt" region="texte1-moon" begin="2s" dur="9s" />
            <text src="textes/GC-duree.txt" region="texte2-moon" dur="10s" />
            <text src="textes/GC-cause.txt" region="texte3-moon" />
          </seq>

          <seq>
            <img src="images/GC-image2.jpg" region="image-moon"  dur="12s" />
            <img src="images/GC-image.jpg" region="image-moon" dur="13s" />
            <video  src="videos/GC-video.mp4" region="video" dur="6s" />
          </seq>

          <seq>
            <audio src="audios/GC-titre.mp3"  />
            <audio src="audios/GC-audio.mp3"  />
          </seq>

       </par>

       <!--pleine lune-->
       <par>
                  
         <text src="textes/pl-titre.txt" region="titre" begin="2s" dur="32s" />

          <seq>
            <text src="textes/pl-apparence.txt" region="texte1-moon"  begin="1s" dur="15s" />
            <text src="textes/pl-duree.txt" region="texte2-moon" dur="10s" />
            <text src="textes/pl-cause.txt" region="texte3-moon"  />
          </seq>
        
          <seq>
            <img src="images/pl-image2.jpg" region="image-moon"  dur="15s" />
            <img src="images/pl-image.jpg" region="image-moon" dur="10s"  />
            <img src="images/pl-image3.png" region="video"   />
          </seq>

          <seq>
           <audio src="audios/pl-titre.mp3"   />
            <audio src="audios/pl-audio.mp3" />
          </seq>

       </par>

       <!--lune gibbeuse decroissante-->
       <par>
          
         <text src="textes/GD-titre.txt" region="titre" begin="2s" dur="35s" />

         <seq>
            <text src="textes/GD-apparence.txt" region="texte1-moon"  begin="1s" dur="15s" />
            <text src="textes/GD-duree.txt" region="texte2-moon" dur="15s" />
            <text src="textes/GD-cause.txt" region="texte3-moon"/>
         </seq>
      
          <seq>
            <img src="images/GD-image.jpg" region="image-moon"    dur="25s" />
            <video src="videos/GD-video.mp4" region="video" />
          </seq>

          <seq>
           <audio src="audios/GD-titre.mp3"  />
           <audio src="audios/GD-audio.mp3"  />
         </seq>
     </par>

      </seq>

    </par>
       

  </body>
</smil>